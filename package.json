{
  "name": "adcs_plot",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/d3-scale": "^1.0.8",
    "@types/d3-shape": "^1.0.10",
    "cycle-scripts-one-fits-all": "2.0.0"
  },
  "dependencies": {
    "@cycle/dom": "^17.1.0",
    "@cycle/http": "^13.2.0",
    "@cycle/isolate": "^2.1.0",
    "@cycle/run": "^3.1.0",
    "@cycle/time": "^0.8.0",
    "cyclejs-utils": "^1.0.3",
    "d3-axis-hyperscript": "0.0.1",
    "d3-scale": "^1.0.5",
    "d3-shape": "^1.0.6",
    "express": "^4.15.2",
    "node-static": "^0.7.9",
    "nodejs-websocket": "^1.7.1",
    "xstream": "^10.3.0"
  },
  "scripts": {
    "start": "cycle-scripts start",
    "test": "cycle-scripts test",
    "build": "cycle-scripts build",
    "eject": "cycle-scripts eject",
    "server": "node server/index.js"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    },
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          },
          "useBuiltIns": true
        }
      ]
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "Snabbdom.createElement"
        }
      ]
    ]
  }
}
